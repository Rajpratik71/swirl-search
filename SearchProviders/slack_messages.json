{
    "name": "Slack Messages (web/Slack.com)",
    "active": true,
    "default": false,
    "connector": "RequestsGet",
    "url": "https://slack.com/api/search.messages",
    "query_template": "{url}?query={query_string}",
    "query_processor": "",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "query_mappings": "",
    "result_processor": "",
    "result_processors": [
        "MappingResultProcessor"
    ],
    "response_mappings": "FOUND=messages.total,RESULTS=messages.matches",
    "result_mappings": "title=text,body='Sent to {channel.name} by {username}: {text}',author=username,url=permalink,date_published=ts,channel.name,channel.is_channel,channel.is_group,channel.is_im,files[*].name,NO_PAYLOAD",
    "results_per_query": 10,
    "credentials": "bearer=<your-slack-user-token>",
    "eval_credentials": "",
    "tags": [
        "Slack",
        "Messages"
    ]
}
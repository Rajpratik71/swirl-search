{
    "name": "Slack Files (web/Slack.com)",
    "active": true,
    "default": false,
    "connector": "RequestsGet",
    "url": "https://slack.com/api/search.files",
    "query_template": "{url}?query={query_string}",
    "query_processor": "",
    "query_processors": [
        "AdaptiveQueryProcessor"
    ],
    "query_mappings": "",
    "result_processor": "",
    "result_processors": [
        "MappingResultProcessor"
    ],
    "response_mappings": "FOUND=files.total,RESULTS=files.matches",
    "result_mappings": "title=title,body=name,author=user,url=permalink,date_published=created,filetype,pretty_type,size,channels,groups,ims,shares[*].private,shares[*].public,NO_PAYLOAD",
    "results_per_query": 10,
    "credentials": "bearer=<your-slack-user-token>",
    "eval_credentials": "",
    "tags": [
        "Slack",
        "Files"
    ]
}